<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/mobile_navigation"
        app:startDestination="@id/navigation_home">
    <fragment
            android:id="@+id/navigation_home"
            android:name="org.jellyfin.mpp.app.ui.home.HomeFragment"
            android:label="@string/title_home"
            tools:layout="@layout/fragment_home" />
    <fragment
            android:id="@+id/navigation_movies"
            android:name="org.jellyfin.mpp.app.ui.movies.MoviesFragment"
            android:label="@string/title_movies"
            tools:layout="@layout/movies_fragment" />
    <fragment
            android:id="@+id/navigation_music"
            android:name="org.jellyfin.mpp.app.ui.music.MusicFragment"
            android:label="@string/title_music"
            tools:layout="@layout/music_fragment" />
</navigation>
